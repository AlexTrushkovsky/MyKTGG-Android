<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle"
            android:padding="5dp">
            <solid android:color="#E9F4EF" />
            <corners
                android:radius="10dp"/>
        </shape>
    </item>
    <item android:bottom="2px" android:right="2px">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            android:padding="5dp">
            <solid android:color="#E9F4EF"/> <!-- this one is ths color of the Rounded Button -->
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</layer-list>